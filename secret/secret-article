  const initWaline = (Fn, el = document, path = window.location.pathname) => {
    const waline = Fn({
      el: el.querySelector('#waline-wrap'),
      serverURL: 'https://waline.yeding.top',
      pageview: false,
      dark: 'html[data-theme="dark"]',
      comment: false,
      ...option,
